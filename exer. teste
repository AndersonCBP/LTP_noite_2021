#include <stdio.h>
 
int main() {
 //Declaração de variaveis
 int num1, num2, soma, multiplicaçõa;
 
 //entradas
 printf("informe o primeiro numero:");
 scanf("%d",&num1);
 printf("informe o segundo numero:");
 scanf("%d",&num2);
 //processamento
 soma = num1 + num2;
 multiplicaçõa = soma*num1;
 //saida
 printf("o resultado da multiplicação é %d",multiplicaçõa);
 printf(" seu corno:");
 
 return 0;
}
